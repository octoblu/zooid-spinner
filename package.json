{
  "name": "zooid-spinner",
  "version": "1.0.6",
  "description": "Spinner",
  "main": "dist/bundle.js",
  "scripts": {
    "build": "webpack --devtool source-map",
    "build:storybook": "build-storybook -o storybook-build",
    "clean": "rimraf lib dist coverage",
    "coverage": "nyc npm test",
    "lint": "npm run lint:js && npm run lint:css",
    "lint:fix": "npm run lint:js:fix && npm run lint:css:fix",
    "lint:js": "eslint src stories",
    "lint:js:fix": "eslint src stories --fix",
    "lint:css": "stylelint ./src/**/*.css",
    "lint:css:fix": "stylefmt ./src/**/*.css",
    "start": "npm run storybook",
    "storybook": "start-storybook -p 9001",
    "test": "mocha",
    "test:watch": "mocha -w -R mocha-multi --reporter-options spec=-,mocha-osx-reporter=-",
    "mocha:json": "mocha --reporter json > coverage/mocha.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/octoblu/zooid-spinner.git"
  },
  "keywords": [
    "zooid",
    "zooid-ui",
    "react",
    "reactjs",
    "octoblu"
  ],
  "author": "Octoblu Inc",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/octoblu/zooid-spinner/issues"
  },
  "homepage": "https://github.com/octoblu/zooid-spinner#readme",
  "nyc": {
    "include": [
      "src"
    ],
    "exclude": [
      "src/**/*.spec.js",
      "src/**/*.css"
    ],
    "cache": true,
    "reporter": [
      "text-summary",
      "lcov",
      "json"
    ],
    "require": [
      "babel-core/register",
      "babel-polyfill"
    ],
    "extension": [
      ".coffee"
    ]
  },
  "devDependencies": {
    "@kadira/react-storybook-addon-info": "^3.4.0",
    "@kadira/storybook": "^2.35.3",
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^6.4.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-register": "^6.24.1",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.28.0",
    "css-modules-require-hook": "^4.0.6",
    "enzyme": "^2.8.1",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^14.1.0",
    "eslint-plugin-babel": "^4.1.1",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "^4.0.0",
    "eslint-plugin-react": "^6.10.3",
    "extract-text-webpack-plugin": "^2.1.0",
    "ignore-styles": "^5.0.1",
    "jsdom": "^9.12.0",
    "mocha": "^3.2.0",
    "mocha-multi": "^0.11.0",
    "mocha-osx-reporter": "^0.1.2",
    "nyc": "^10.2.0",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.5.1",
    "react-dom": "^15.5.4",
    "react-test-renderer": "^15.5.4",
    "rimraf": "^2.6.1",
    "sinon": "^2.1.0",
    "sinon-chai": "^2.9.0",
    "style-loader": "^0.16.1",
    "stylefmt": "^5.3.2",
    "stylelint": "^7.10.1",
    "stylelint-config-standard": "^16.0.0",
    "webpack": "^2.3.3"
  },
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.5.8",
    "react": "^15.5.4"
  }
}
